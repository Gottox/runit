#!/bin/sh

PATH=/command:/usr/local/bin:/usr/local/sbin:/bin:/sbin:/usr/bin:/usr/sbin:/usr/X11R6/bin

exec </dev/null
exec >/dev/null
exec 2>/dev/null

/command/svc -x /service/*

exec env - PATH=$PATH \
runsvdir /service log:_...........................................................................................................................................................................................................................................................................................................................................................................................................
